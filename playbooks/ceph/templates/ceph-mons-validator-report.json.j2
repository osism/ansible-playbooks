{
  "validator": "ceph-mons",
  "validator_result": "{{ ceph_validate_mons_result }}",
  "validator_reason": {{ ceph_validate_mons_reasons|to_json }},
  "validator_tests": [
    {% for test in ceph_validate_mons_tests %}
    {
      "test_name": "{{ test.name }}",
      "test_result": "{{ test.result }}",
      "test_data": {{ test.data }}
    }{%- if not loop.last -%},{% endif %}
    {% endfor %}
  ]
}
